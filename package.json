{
  "name": "adminizer",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "homepage": "https://YOUR_USERNAME.github.io/adminizer-demo",
  "scripts": {
    "dev": "vite",
    "dev:demo": "vite --mode demo",
    "dev:production": "vite --mode production",
    "build": "tsc && vite build",
    "build:demo": "tsc && vite build --mode demo",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:demo": "vite preview --mode demo",
    "preview:production": "vite preview --mode production",
    "deploy:demo": "npm run build:demo && gh-pages -d dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
